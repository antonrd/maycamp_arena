<h1>Add problem to contest <%= @contest.name %></h1>
<% form_for [:admin, @contest, @problem] do |f| -%>
  <p>
    <%= f.error_messages %>
  </p>
  <%= render :partial => 'problem_fields', :locals => { :f => f } %>
  <p>
    <%= f.submit "Create" %><%= link_to "Cancel", edit_admin_contest_path(@contest) %>
  </p>
<% end -%>