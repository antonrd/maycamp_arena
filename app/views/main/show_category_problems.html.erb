<h2><%= h(@category.name) %> - Задачи</h2>
<%= link_to 'Списък с всички категории', :controller => :main, :action => :problems_categories %>
<br><br>
<% for @cat_prob in @category.problems do %>
  <% if @cat_prob.contest.practicable and @cat_prob.contest.visible then %>
    <%= link_to @cat_prob.contest.name + " / " + @cat_prob.name, practice_path(:action => :open_contest, :contest_id => @cat_prob.contest_id) %><br>
  <% end %>
<% end %>

